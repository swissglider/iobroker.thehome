    "name": "Swissglider",
    "email": "npm@mailschweiz.com"
  },
  "homepage": "https://github.com/swissglider/ioBroker.thehome",
  "license": "MIT",
  "keywords": [
    "config",
    "swissglider",
    "influxdb",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swissglider/ioBroker.thehome"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.2",
    "@influxdata/influxdb-client": "^1.20.0",
    "@influxdata/influxdb-client-apis": "^1.18.0",
    "@iobroker/adapter-core": "^2.5.1",
    "@iobroker/adapter-react": "2.0.14",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.5",
    "@mui/styles": "^5.0.2",
    "@mui/x-data-grid": "^4.0.1",
    "ajv": "^8.6.3",
    "babel-polyfill": "^6.26.0",
    "fg-loadcss": "^3.1.0",
    "fp-ts": "^2.11.4",
    "grommet": "^2.18.0",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "material-ui-dropzone": "^3.5.0",
    "miio-token-extractor": "^1.0.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "recoil": "^0.4.1",
    "regenerator-runtime": "^0.13.9",
