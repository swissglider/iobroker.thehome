    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.16.0",
    "@iobroker/testing": "^2.5.2",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.0",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "gulp": "^4.0.2",
    "mocha": "^9.1.2",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.4.1",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "main": "build/main.js",
  "targets": {
    "main": false,
    "admin": {
      "distDir": "admin/build"
    },
    "www": {
      "distDir": "www/js"
    }
  },
  "scripts": {
    "copyReactClientApp": "sh ./script/copyReactClientApp.sh",
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx admin/src/tab.tsx --target admin",
    "build:www": "npm run copyReactClientApp && parcel build www/src/index.tsx --target www",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel && npm run build:www",
    "watch:parcel": "parcel admin/src/index.tsx admin/src/tab.tsx --target admin --dist-dir admin/build",
    "watch:www": "parcel www/src/index.tsx --target www --dist-dir www/js",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/ www/src/",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/swissglider/ioBroker.thehome/issues"
  },
  "readmeFilename": "README.md"
}
